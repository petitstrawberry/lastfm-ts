{
    "name": "@petitstrawberry/lastfm-ts",
    "version": "2.0.0",
    "description": "A minimalistic TypeScript-based client for Last.fm API 2.0",
    "author": "Mikhail Prosmitskiy",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/petitstrawberry/lastfm-ts.git"
    },
    "main": "dist/index.min.js",
    "types": "dist/index.min.d.js",
    "scripts": {
        "build": "rollup --config rollup.config.ts --configPlugin rollup-plugin-ts",
        "lint": "eslint src/",
        "test": "jest",
        "format": "prettier . --write --log-level warn"
    },
    "keywords": [
        "lastfm",
        "typescript",
        "api"
    ],
    "license": "MIT",
    "devDependencies": {
        "@rollup/plugin-terser": "^0.4.4",
        "@types/eslint": "^8",
        "@types/jest": "^29.5.11",
        "@types/node": "^20.10.5",
        "@typescript-eslint/eslint-plugin": "^6.16.0",
        "@typescript-eslint/parser": "^6.16.0",
        "eslint": "^8.56.0",
        "jest": "^29.7.0",
        "prettier": "^3.1.1",
        "rollup": "^4.9.1",
        "rollup-plugin-ts": "^3.4.5",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.2",
        "typescript": "^5.3.3"
    },
    "packageManager": "yarn@4.0.2",
    "engines": {
        "node": ">= 20.10.0"
    }
}
